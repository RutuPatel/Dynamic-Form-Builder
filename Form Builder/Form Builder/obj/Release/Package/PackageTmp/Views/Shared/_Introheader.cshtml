<div class="intro-header" id="home">

    <div class="col-xs-12 text-center abcen1">
        <h1 class="h1_home wow fadeIn" data-wow-delay="0.4s">Form Builder</h1>
        <h3 class="h3_home wow fadeIn" data-wow-delay="0.6s">Your Choice Your Form</h3>
        @if (Session["UserEmail"] != null)
        {
            <h3 class="h3_home wow fadeIn" data-wow-delay="0.6s">
                <text>
                    Welcome @Session["UserName"].ToString() !!  
                </text>
            </h3>
            <ul class="list-inline intro-social-buttons">
                <li>
                    <a href="@Url.Action("Fields","Home")" class="btn  btn-lg mybutton_cyano wow fadeIn" data-wow-delay="0.8s"><span class="network-name">Try Our Builder</span></a>
                </li>
                <li id="download">
                    <a href="@Url.Action("LogOut","Home")" class="btn  btn-lg mybutton_standard wow swing wow fadeIn" data-wow-delay="1.2s"><span class="network-name">Sign Out</span></a>
                </li>
            </ul>
        }
        else
        {
            <ul class="list-inline intro-social-buttons">
                <li>
                    <a href="@Url.Action("SignUp","Home")" class="btn  btn-lg mybutton_cyano wow fadeIn" data-wow-delay="0.8s"><span class="network-name">Create An Account</span></a>
                </li>
                <li id="download">
                    <a href="@Url.Action("Login","Home")" class="btn  btn-lg mybutton_standard wow swing wow fadeIn" data-wow-delay="1.2s"><span class="network-name">Login</span></a>
                </li>
            </ul>
        }
    </div>
    <!-- /.container -->
    <div class="col-xs-12 text-center abcen wow fadeIn">
        <div class="button_down ">
            <a class="imgcircle wow bounceInUp" data-wow-duration="1.5s" href="#howitwork"> <img class="img_scroll" src="@Url.Content("~/Resources/img/icon/circle.png")" alt=""> </a>
        </div>
    </div>
</div>